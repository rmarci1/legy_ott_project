{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "./app/**/*.jsx",
    "nativewind-env.d.ts",
    "./constants/**/*.js",
    "./context/**/*.js",
    "./lib/**/*.js",
    "lib/api.js"
  ]
}
